{
  "name": "mh091rc1a",
  "version": "0.9.1",
  "description": "Makehuman 0.9.1-rc1a as WebApp",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:data && npm run build:once && npm run build:chai",
    "build:once": "rollup --config rollup.config.mjs",
    "build:chai": "mkdir -p build/test/chai && cp test/chai/*\\.js test/chai/*\\.d\\.ts build/test/chai",
    "build:data": "test -f data/directory.json || ts-node src/filesystem/build-data-dir.ts",
    "rebuild:data": "rm -f data/directory.json && npm run build:data",
    "build:watch": "rollup --config rollup.config.js --watch",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "build:client": "rollup --bundleConfigAsCjs --config rollup-client.config.js",
    "dev:prepare": "npm run build:data && npm run build:chai",
    "dev:build": "tspc -w -p tsconfig.dev.json",
    "dev:test": "if test -z \"$npm_config_file\" ; then wtr --watch 'build/test/**/*.spec.js' ; else wtr --watch \"$npm_config_file\" ; fi",
    "dev:serve": "wds --node-resolve --watch --open --app-index index.dev.html",
    "update": "npx npm-check-updates -u && npm install"
  },
  "author": "Mark-Andr√© Hopf <mhopf@mark13.org> based on work of the MakeHuman Team (www.makehumancommunity.org)",
  "license": "GPL-2.0-or-later",
  "engines": {
    "npm": ">=8.1.2",
    "node": ">=16.10.0"
  },
  "dependencies": {
    "fflate": "^0.8.1",
    "gl-matrix": "^3.4.3",
    "toad.js": "^0.1.7",
    "toad.jsx": "^0.0.10"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@types/chai": "^4.3.11",
    "@types/chai-almost": "^1.0.3",
    "@types/chai-string": "^1.4.5",
    "@types/mocha": "^10.0.5",
    "@types/node": "^20.9.2",
    "@web/dev-server": "^0.4.1",
    "@web/test-runner": "^0.18.0",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "typescript": "^5.2.2"
  }
}
